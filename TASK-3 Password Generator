import string
import random

if __name__ == "__main__":
    string1 = string.ascii_lowercase
    string2 = string.ascii_uppercase
    string3 = string.digits
    string4 = string.punctuation
    pass_length = int(input("Enter length of your desired password \n"))
    string = []
    string.extend(list(string1))
    string.extend(list(string2))
    string.extend(list(string3))
    string.extend(list(string4))
    print("Your password is: ")
    print("".join(random.sample(string, pass_length)))


